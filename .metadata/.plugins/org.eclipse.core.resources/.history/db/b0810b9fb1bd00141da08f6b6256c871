package se.kth.ict.id2203.components.multipaxos;

import se.kth.ict.id2203.ports.fpl.FplDeliver;
import se.sics.kompics.address.Address;

public class Prepare extends FplDeliver{

	private static final long serialVersionUID = -2581064400982342179L;
	private int pts;
	private int al;
	private int t;
	protected Prepare(Address source, int pts, int al, int t) {
		super(source);
		this.pts = pts;
		this.al = al;
		this.t = t;
	}
	public int getTs(){
		return pts;
	}
	public int getAl(){
		return al;
	}
	public int getT(){
		return t;
	}

}
